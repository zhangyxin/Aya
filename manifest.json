{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "tabs",
        "scripting",
        "bookmarks"
    ],
    "optional_permissions": [],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "optional_host_permissions": [],
    "devtools_page": "index.html",
    "background": {
        "service_worker": "bg-wrapper.js"
    },
    "action": {
        "default_title": "Default Title",
        "default_popup": "popup/popup.html",
        "default_icon": {
            "32": "icons/32.png",
            "72": "icons/72.png",
            "128": "icons/128.png",
            "512": "icons/512.png"
        }
    },
    "icons": {
        "32": "icons/32.png",
        "72": "icons/72.png",
        "128": "icons/128.png",
        "512": "icons/512.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://developer.chrome.com/docs/extensions/*",
                "https://developer.chrome.com/docs/webstore/*"
            ]
        }
    ],
    "manifest_version": 3
}